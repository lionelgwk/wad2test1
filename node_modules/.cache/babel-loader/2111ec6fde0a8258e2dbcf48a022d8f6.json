   console.log(partyLeader);\n      console.log(location);\n      console.log(friends);\n      console.log(eventStatus);\n      }\n    }\n  }\n\n</script>\n\n<style>\n\n#largefiller {\n  height: 100px;\n}\n\n\n#filler {\n  height: 20px;\n}\n\ninput[type=\"date\"] {\n  border-radius: 10px;\n  padding:10px;\n  font-family: 'Varela Round', sans-serif;\n}\n\n\ninput[type=\"text\"] {\n  border: 0;\n  outline: 0;\n  background: transparent;\n  border-bottom: 1px solid black;\n  font-family: 'Varela Round', sans-serif;\n  font-size:40px;\n  text-align: center;\n}\n\n\n.button {\n  background-color: crimson;\n  font-family: 'Varela Round', sans-serif;\n  color:white;\n  border-radius:10px;\n  margin-left:10px\n}\n\nselect {\n  width: 150px;\n  font-family: 'Varela Round', sans-serif;\n  height: 40px;\n  border-radius: 10px;\n  padding:5px;\n  margin-right:10px;\n}\n\n.row {\n  margin-bottom:10px\n}\n\n\n.toast-wrapper {\n    position: fixed;\n    width: 100%;\n    top: 20px;\n  }\n.toast {\n  padding: 20px;\n  color: white;\n  background: #ff0062;\n  border-radius: 10px;\n  box-shadow: 1px 3px 5px rgba(0,0,0,0.2);\n  max-width: 400px;\n  margin: 0 auto;\n}\n\n\n/* .toast-enter-from {\n  opacity: 0;\n  transform: translateY(-60px);\n}\n\n.toast-enter-to {\n  opacity: 1;\n  transform: translateY(0);\n}\n\n.toast-enter-active {\n  transition: all 0.3s ease;\n} */\n\n</style>"]},"metadata":{},"sourceType":"module"}