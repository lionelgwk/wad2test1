{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport db from '@/fb';\nexport default {\n  data() {\n    return {\n      friends: [],\n      selectedFriends: []\n    };\n  },\n  emits: ['add-friend'],\n  methods: {\n    sortBy(prop) {\n      this.friends.sort((a, b) => a[prop] < b[prop] ? -1 : 1);\n    },\n    addFriend(friend) {\n      this.$emit('add-friend', friend);\n      this.selectedFriends.push(friend);\n    }\n  },\n  mounted() {\n    db.collection('users').onSnapshot(res => {\n      const changes = res.docChanges();\n      changes.forEach(change => {\n        if (change.type === 'added') {\n          this.friends.push({\n            ...change.doc.data(),\n            id: change.doc.id\n          });\n        }\n      });\n    });\n    console.log(this.friends);\n  }\n};","map":{"version":3,"mappings":";AAsBA,OAAOA,EAAC,MAAO,MAAM;AAErB,eAAc;EACVC,IAAI,GAAE;IACF,OAAO;MACHC,OAAO,EAAE,EAAE;MACXC,eAAe,EAAE;IACrB;EACJ,CAAC;EACDC,KAAK,EAAE,CAAC,YAAY,CAAC;EACrBC,OAAO,EAAC;IACJC,MAAM,CAACC,IAAI,EAAC;MACR,IAAI,CAACL,OAAO,CAACM,IAAI,CAAC,CAACC,CAAC,EAACC,CAAC,KAAKD,CAAC,CAACF,IAAI,IAAIG,CAAC,CAACH,IAAI,IAAI,CAAC,IAAI,CAAC;IACzD,CAAC;IACDI,SAAS,CAACC,MAAM,EAAC;MACb,IAAI,CAACC,KAAK,CAAC,YAAY,EAAED,MAAM,CAAC;MAChC,IAAI,CAACT,eAAe,CAACW,IAAI,CAACF,MAAM,CAAC;IACrC;EACJ,CAAC;EACDG,OAAO,GAAE;IACLf,EAAE,CAACgB,UAAU,CAAC,OAAO,CAAC,CAACC,UAAU,CAACC,GAAE,IAAK;MACrC,MAAMC,OAAM,GAAID,GAAG,CAACE,UAAU,EAAE;MAChCD,OAAO,CAACE,OAAO,CAACC,MAAK,IAAK;QACtB,IAAIA,MAAM,CAACC,IAAG,KAAM,OAAO,EAAC;UACxB,IAAI,CAACrB,OAAO,CAACY,IAAI,CAAC;YACd,GAAGQ,MAAM,CAACE,GAAG,CAACvB,IAAI,EAAE;YACpBwB,EAAE,EAAEH,MAAM,CAACE,GAAG,CAACC;UACnB,CAAC;QACL;MACJ,CAAC;IACL,CAAC;IACDC,OAAO,CAACC,GAAG,CAAC,IAAI,CAACzB,OAAO;EAC5B;AACJ","names":["db","data","friends","selectedFriends","emits","methods","sortBy","prop","sort","a","b","addFriend","friend","$emit","push","mounted","collection","onSnapshot","res","changes","docChanges","forEach","change","type","doc","id","console","log"],"sourceRoot":"",