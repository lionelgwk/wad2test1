onMounted(async () => {\n        await loader.load()\n        map = new google.maps.Map(mapDiv.value, {\n          center: currPos.value,\n          zoom: 15\n        })\n        \n      }),\n\n      watch(currPos, () => {\n        map.setCenter(currPos.value)\n      })\n\n\n\n\n\n      return {currPos, mapDiv}\n    }\n  });\n  \n\n</script>\n  "]},"metadata":{},"sourceType":"module"}